{
  "name": "notion2ical",
  "version": "0.0.3",
  "description": "A library that converts a Notion calendar database into iCalendar \\(.ics\\) format.",
  "keywords": [
    "ical",
    "ics",
    "calendar",
    "export",
    "sync",
    "converter",
    "notion2ical",
    "notion",
    "notion ics",
    "notion ical",
    "notion to ical",
    "notion to ics",
    "notion2ical",
    "notion calendar",
    "notion export",
    "notion sync",
    "notion integration",
    "notion to ical",
    "notion to ics"
  ],
  "author": "brenank",
  "license": "MIT",
  "packageManager": "npm@22.16.0",
  "repository": "brenank/notion2ical",
  "type": "module",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=22.16.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "tsc -b",
    "build": "tsc -p tsconfig.build.json",
    "test": "tsx --tsconfig tsconfig.test.json --test '**/*.test.ts'",
    "test:updateexpected": "UPDATE_EXPECTED=1 npm run test",
    "test:updatemocks": "UPDATE_MOCKS=1 npm run test:updateexpected",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "verify": "npm run check && npm run test && npm run lint && npm run fmt",
    "checkchange": "beachball check",
    "change": "beachball change",
    "release": "beachball publish"
  },
  "dependencies": {
    "@notionhq/client": "^3.1.3",
    "ics": "^3.8.1"
  },
  "peerDependencies": {
    "@notionhq/client": "^3.1.3"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "beachball": "^2.54.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.2",
    "eslint-plugin-import-x": "^4.15.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "nock": "^14.0.5",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
